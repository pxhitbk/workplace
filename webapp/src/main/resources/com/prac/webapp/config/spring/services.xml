<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="authentication" class="com.prac.service.impl.AuthenticationServiceImpl"/>
	
	<bean id="serviceResolver" factory-method="createServiceResolver" class="com.prac.util.ServiceResolver">
		<constructor-arg>
			<map>
				<entry key="com.prac.service.AuthenticationService" value-ref="authentication"/>
			</map>
		</constructor-arg>
	</bean>
</beans>
